{
  "name": "Pomponiarz760",
  "description": "Najbardziej byczkowe rozszerzenie do ttv.",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
	"default_icon": "icon32.png",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
  }
}