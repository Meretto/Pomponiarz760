{
	"name": "Pomponiarz760",
	"description": "Najbardziej byczkowe rozszerzenie do ttv.",
	"version": "0.1",
	"manifest_version": 2,
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"background": {
		"scripts": ["js/background.js"]
	},
	"permissions": [
		"activeTab",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": [
				"https://*.twitch.tv/*"
			],
			"js": [
				"js/content.js"
			]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "img/icon32.png",
		"icons": {
			"16": "img/icon16.png",
			"48": "img/icon48.png",
			"128": "img/icon128.png"
		}
	}
}